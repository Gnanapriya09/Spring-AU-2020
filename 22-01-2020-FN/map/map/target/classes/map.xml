<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="employeeinfo" class="com.SpringAU.info.EmployeeInfo">
		<constructor-arg name="employee">
			<map>
				<entry key="index">
					<bean id="employee" class="com.SpringAU.pojo.Employee">

						<constructor-arg value="09"></constructor-arg>
						<constructor-arg value="Kayal" type="String"></constructor-arg>
						<constructor-arg value="Software Engineer"
							type="String"></constructor-arg>
					</bean>
				</entry>
			</map>
		</constructor-arg>
	</bean>
</beans>